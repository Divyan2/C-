#include <iostream>

class Employee
{
private:
  std::string Name;
  std::string Company;
  int Age;
public:
void setName(std::string name){
    Name = name;
}
std::string getName(){
    return Name;
}
void setCompany(std::string company){
    Company = company;
}
std::string getCompany(){
    return Company;
}
void setAge(int age){
    Age = age;
}
int getAge(){
    return Age;
}
void intro(){
  std::cout<<Name<<std::endl;
  std::cout<<Company<<std::endl;
  std::cout<<Age<<std::endl;
}
Employee(std::string name,std::string company,int age){
  Name = name;
  Company = company;
  Age = age;
}
};

int main() {
 Employee employee1 = Employee("Divyan","NASA",25);
 Employee employee2 = Employee("Chandru","Isro",30);
 employee1.setAge(24);
 std::cout<<employee1.getAge()<<std::endl;
 employee1.intro();
 return 0;
}
